  element ns1:g {
    text,
    tei_att.global.responsibility.attribute.cert,
    tei_att.global.responsibility.attribute.resp,
    
    ## indica la descripción del carácter o pictograma dado.
    attribute ref { tei_data.pointer }?,
    empty
  }
tei_charProp =
  
  ## (propiedad de carácter) proporciona un nombre y un valor para alguna propiedad del carácter o pictograma del padre. [5.2. Markup Constructs for Representation of Characters and Glyphs]
  element ns1:charProp {
    ((tei_localName), tei_value),
    tei_att.global.responsibility.attribute.cert,
    tei_att.global.responsibility.attribute.resp,
