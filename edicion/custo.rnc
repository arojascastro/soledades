    tei_att.global.responsibility.attribute.cert,
    tei_att.global.responsibility.attribute.resp,
    empty
  }
tei_origDate =
  
  ## (fecha origen) contiene cualquier tipo de fecha utilizada para indicar la fecha de origen de un manuscrito o de una de sus partes. [10.3.1. Origination]
  element ns1:origDate {
    (text | tei_model.gLike | tei_model.phrase | tei_model.global)*,
    tei_att.datable.w3c.attribute.when,
    tei_att.datable.w3c.attribute.notBefore,
    tei_att.datable.w3c.attribute.notAfter,
    empty
  }
tei_origPlace =
  
  ## (lugar de origen) contiene cualquier tipo de nombre de lugar utilizado para indicar el lugar de origen de un manuscrito o de una de sus partes. [10.3.1. Origination]
  element ns1:origPlace { tei_macro.phraseSeq }
tei_msIdentifier =
  
  ## (identificador del manuscrito) contiene la información necesaria para identificar el manuscrito que se examina. [10.4. The Manuscript Identifier]
  element ns1:msIdentifier {
    ((tei_model.placeNamePart_sequenceOptional,
      tei_repository?,
      tei_collection*,
      tei_idno?),
     (tei_msName | tei_altIdentifier)*)
    >> sch:pattern [
         id = "custo-msIdentifier-msId_minimal-constraint-8"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:msIdentifier"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "not(parent::tei:msPart) and       (local-name(*[1])='idno' or       local-name(*[1])='altIdentifier' or       normalize-space(.)='')"
             "An msIdentifier must contain either a repository or location of some type, or a manuscript name"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    tei_att.global.responsibility.attribute.cert,
    tei_att.global.responsibility.attribute.resp,
    empty
  }
